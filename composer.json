{
    "name": "jschreuder/middle-di",
    "description": "A typehintable dependency injection container.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "jschreuder\\MiddleDi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jelmer Schreuder",
            "email": "github@jelmerschreuder.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    }
}
